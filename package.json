{
  "name": "@davisuga/rescript-nativebase",
  "description": "ReScript bindings for react-native-XXXXXXXXXX",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@rescript/react": ">=0.10.0",
    "native-base": "^3.4.13",
    "rescript-react-native": ">=0.68.0"
  },
  "overrides": {
    "react": "17.0.2"
  },
  "repository": "https://github.com/davisuga/rescript-nativebase.git",
  "license": "MIT",
  "keywords": [
    "rescript",
    "react-native",
    "react",
    "nativebase"
  ],
  "files": [
    "*.md",
    "bsconfig.json",
    "src/**/*.res",
    "src/**/*.resi",
    "src/**/*.js",
    "!src/**/*.bs.js"
  ],
  "scripts": {
    "format:most": "prettier --write \"**/*.{md,json,js,css}\"",
    "format:res": "rescript format -all",
    "format": "npm run format:most && npm run format:res",
    "re:start": "rescript build -w",
    "re:build": "rescript",
    "re:clean-build": "rescript clean && rescript",
    "start": "npm run re:start",
    "build": "npm run re:build",
    "test": "npm run re:clean-build",
    "release": "npmpub"
  },
  "devDependencies": {
    "@greenlabs/ppx-ts": "^0.1.6",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.6",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "npmpub": "^5.0.0",
    "prettier": "^2.0.0",
    "rescript": "10"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{md,json,js,css}": "prettier --write",
    "*.{res,resi}": "rescript format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "native-base": "^3.4.13",
    "rescript-react-native": "^0.68.1"
  }
}
