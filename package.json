{
  "name": "rescript-nativebase",
  "description": "ReScript bindings for NativeBase",
  "version": "0.0.2",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@rescript/react": ">=0.10.0",
    "native-base": "^3.4.13",
    "rescript-react-native": ">=0.68.0"
  },
  "overrides": {
    "react": "17.0.2"
  },
  "repository": "https://github.com/davisuga/rescript-nativebase.git",
  "license": "MIT",
  "keywords": [
    "rescript",
    "react-native",
    "react",
    "nativebase"
  ],
  "files": [
    "*.md",
    "bsconfig.json",
    "src/**/*.res",
    "src/**/*.resi",
    "src/**/*.js",
    "!src/**/*.bs.js"
  ],
  "scripts": {
    "format:most": "prettier --write \"**/*.{md,json,js,css}\"",
    "format:res": "rescript format -all",
    "format": "npm run format:most && npm run format:res",
    "re:start": "rescript build -w",
    "re:build": "rescript",
    "re:clean-build": "rescript clean && rescript",
    "start": "npm run re:start",
    "build": "npm run re:build",
    "test": "npm run re:clean-build",
    "release": "npmpub",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@greenlabs/ppx-ts": "^0.1.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.6",
    "babel-loader": "^8.2.5",
    "fluture": "^14.0.0",
    "fp-ts": "^2.12.3",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "npmpub": "^5.0.0",
    "prettier": "^2.0.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "rescript": "10",
    "reshowcase": "^5.2.0",
    "storybook-addon-swc": "^1.1.8",
    "typescript": "^4.8.2",
    "vite": "^3.1.0"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{md,json,js,css}": "prettier --write",
    "*.{res,resi}": "rescript format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "@storybook/addon-react-native-web": "^0.0.18",
    "bs-css": "^15.3.0",
    "bs-css-emotion": "^4.3.0",
    "native-base": "^3.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rescript-react-native": "^0.68.1"
  }
}
